{
	"name": "action-automerge-release",
	"version": "0.0.0",
	"private": true,
	"description": "Automatically merge changes from a prevous to next releases and the default branch",
	"main": "lib/main.js",
	"scripts": {
		"build": "ncc build ./src/index.ts",
		"package": "ncc build --source-map",
		"all": "npm run build && npm run format && npm run package",
		"push": "yarn all && git add -A && git commit -m 'commit' && git push --force"
	},
	"keywords": [
		"actions",
		"node",
		"setup"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@actions/core": "^1.2.6",
		"@actions/github": "^5.0.0"
	},
	"devDependencies": {
		"@types/jest": "^26.0.20",
		"@types/node": "^15.12.0",
		"@typescript-eslint/parser": "^5.40.0",
		"@zeit/ncc": "^0.22.3",
		"eslint": "^7.21.0",
		"eslint-plugin-github": "^4.1.1",
		"eslint-plugin-jest": "^24.1.5",
		"jest": "^24.9.0",
		"jest-circus": "^29.2.1",
		"prettier": "2.7.1",
		"js-yaml": "^4.1.0",
		"ts-jest": "^24.3.0",
		"typescript": "^4.8.4"
	},
	"peerDependencies": {
		"@octokit/types": "^5.2.0"
	}
}
